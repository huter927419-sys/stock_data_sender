<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!-- MQ消息队列配置（虚拟机端使用，发送数据到宿主机） -->
    <!-- VirtualBox NAT模式配置：
         推荐使用 10.0.2.2（VirtualBox NAT网关IP，直接指向宿主机，无需端口转发）
         详细配置请参考 VirtualBox_NAT配置指南.md
    -->
    <add key="MQHost" value="10.0.2.2" />
    <!-- MQ服务器端口 -->
    <add key="MQPort" value="5678" />
    <!-- 队列名称（日线数据） -->
    <add key="MQQueueName" value="daily_data_queue" />
    <!-- 实时数据队列名称 -->
    <add key="MQRealtimeQueueName" value="realtime_data_queue" />
    <!-- 除权数据队列名称 -->
    <add key="MQExRightsQueueName" value="ex_rights_data_queue" />
    <!-- 码表数据队列名称 -->
    <add key="MQMarketTableQueueName" value="market_table_queue" />
    <!-- 是否启用MQ发送 -->
    <add key="MQEnabled" value="true" />
    <!-- 连接超时时间（毫秒） -->
    <add key="MQConnectTimeout" value="5000" />
    <!-- 发送超时时间（毫秒） -->
    <add key="MQSendTimeout" value="10000" />
  </appSettings>
</configuration>

